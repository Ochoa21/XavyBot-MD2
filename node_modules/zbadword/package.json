    "type": "git",
    "url": "git+https://github.com/srart24/ZBadWord.git"
  },
  "bugs": {
    "url": "https://github.com/srart24/ZBadWord/issues"
  },
  "homepage": "https://github.com/srart24/ZBadWord",
  "devDependencies": {
    "@types/node": "^20.14.13",
    "typescript": "^5.5.4"
  }
